{
  "name": "@southern-syntax/schemas",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./language": {
      "types": "./dist/language.d.ts",
      "import": "./dist/language.js",
      "require": "./dist/language.js",
      "default": "./dist/language.js"
    },
    "./media": {
      "types": "./dist/media.d.ts",
      "import": "./dist/media.js",
      "default": "./dist/media.js"
    },
    "./media-taxonomy": {
      "types": "./dist/media-taxonomy.d.ts",
      "import": "./dist/media-taxonomy.js",
      "default": "./dist/media-taxonomy.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc -b --pretty false",
    "build": "tsc -b",
    "check-types": "pnpm typecheck",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@southern-syntax/config": "workspace:*",
    "@southern-syntax/i18n": "workspace:*",
    "@southern-syntax/types": "workspace:*",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@southern-syntax/tsconfig": "workspace:*",
    "typescript": "^5.8.3"
  }
}
