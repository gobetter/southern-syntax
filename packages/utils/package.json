{
  "name": "@southern-syntax/utils",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "check-types": "pnpm typecheck",
    "lint": "eslint . --max-warnings=0",
    "clean": "rimraf dist",
    "typecheck": "tsc -b --pretty false"
  },
  "dependencies": {
    "@southern-syntax/i18n": "workspace:*",
    "@southern-syntax/types": "workspace:*",
    "@southern-syntax/config": "workspace:*",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@southern-syntax/tsconfig": "workspace:*",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  }
}
