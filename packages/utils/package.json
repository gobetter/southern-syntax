{
  "name": "@southern-syntax/utils",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "@southern-syntax/i18n": "workspace:*",
    "@southern-syntax/types": "workspace:*",
    "@southern-syntax/config": "workspace:*",
    "@supabase/supabase-js": "^2.50.0",
    "busboy": "^1.6.0",
    "slugify": "^1.6.6",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@southern-syntax/tsconfig": "workspace:*",
    "@types/busboy": "^1.5.4",
    "@types/node": "^24.2.1",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  }
}
