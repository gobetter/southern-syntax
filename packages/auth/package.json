{
  "name": "@southern-syntax/auth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "default": "./dist/constants.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "default": "./dist/server.js"
    },
    "./service": {
      "types": "./dist/service.d.ts",
      "imort": "./dist/service.js",
      "default": "./dist/service.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.js",
      "default": "./dist/schemas.js"
    },
    "./permissions": {
      "types": "./dist/permissions.d.ts",
      "import": "./dist/permissions.js",
      "default": "./dist/permissions.js"
    },
    "./options": {
      "types": "./dist/options.d.ts",
      "import": "./dist/options.js",
      "default": "./dist/options.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc -b --pretty false",
    "build": "tsc -b",
    "check-types": "pnpm typecheck"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@southern-syntax/config": "workspace:*",
    "@southern-syntax/db": "workspace:*",
    "@southern-syntax/i18n": "workspace:*",
    "@southern-syntax/schemas": "workspace:*",
    "@southern-syntax/types": "workspace:*",
    "bcryptjs": "^2.4.3",
    "next-auth": "4.24.11",
    "server-only": "^0.0.1",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@southern-syntax/eslint-config-custom": "workspace:*",
    "@southern-syntax/tsconfig": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.2.1",
    "typescript": "^5.8.3"
  }
}
