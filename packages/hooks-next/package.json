{
  "name": "@southern-syntax/hooks-next",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./use-toast": {
      "types": "./dist/use-toast.d.ts",
      "import": "./dist/use-toast.js",
      "default": "./dist/use-toast.js"
    },
    "./use-update-query": {
      "types": "./dist/use-update-query.d.ts",
      "import": "./dist/use-update-query.js",
      "default": "./dist/use-update-query.js"
    },
    "./use-upload-media": {
      "types": "./dist/use-upload-media.d.ts",
      "import": "./dist/use-upload-media.js",
      "default": "./dist/use-upload-media.js"
    }
  },
  "types": "dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc -b",
    "typecheck": "tsc -b --pretty false",
    "lint": "eslint . --max-warnings=0",
    "prepack": "pnpm build"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "next": ">=14",
    "next-intl": ">=4",
    "sonner": ">=2"
  },
  "dependencies": {
    "@southern-syntax/types": "workspace:*",
    "@southern-syntax/utils": "workspace:*"
  },
  "devDependencies": {
    "@southern-syntax/tsconfig": "workspace:*",
    "@types/react": "^19.0.0",
    "typescript": "^5.8.3"
  }
}
